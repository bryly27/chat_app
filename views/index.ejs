<html>
<head>
	<title>Chat App</title>
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<script src="http://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
	<script type="text/javascript" src="/socket.io/socket.io.js"></script>
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css">
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap-theme.min.css">
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>
	<script type="text/javascript" src='chat.js'></script>
	<link rel="stylesheet" type="text/css" href="style.css">
</head>
<body>

	<div id='login' class='container-fluid'>
		<h1>Chat App</h1>
		<form>
			<input id='login_name' type='text' placeholder='Enter Name'><br>
			<input id='login_submit' type='submit' value='Enter'>
		</form>
	</div>

	<div id='chatbox'>
		<div class='row header'>
			<div class='col-md-12 col-xs-12'>
				<h1>Chat App</h1>
			</div>
		</div>
		<div id='chat'></div>

		<form id='message_form' class='col-md-12 col-xs-12'>
			<input id='message' type='text' placeholder='enter your message here'>
			<input id='submit' type='submit' value='submit'>
		</form>
	</div>

	
</body>
</html>